<?xml version="1.0" encoding="UTF-8"?>

<db xmlns="http://hongs-core"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://hongs-core db.xsd">
    <config link="manage"/>
    <tables>
        <table name="room"
               tableName="a_mesage_room"
               primaryKey="id">
            <assoc type="HAS_MANY"
                   name="users"
                   tableName="room_mate"
                   foreignKey="rid">
            </assoc>
        </table>
        <table name="note"
               tableName="a_mesage_note"
               primaryKey="id">
        </table>
        <table name="room_mate"
               tableName="a_mesage_room_mate"/>
        <table name="user_mate"
               tableName="a_mesage_user_mate"/>
        <!-- 其他表 -->
        <table name="user"
               tableName="a_member_user"
               primaryKey="id">
        </table>
    </tables>
</db>
